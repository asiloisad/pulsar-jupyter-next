{
  "name": "jupyter-next",
  "description": "Jupyter notebook support with interactive computing, kernel management, and rich output rendering.",
  "repository": "https://github.com/asiloisad/pulsar-jupyter-next",
  "keywords": [
    "jupyter",
    "notebook",
    "ipynb",
    "python",
    "interactive",
    "kernel",
    "data-science",
    "visualization"
  ],
  "version": "0.2.2",
  "license": "MIT",
  "engines": {
    "atom": ">=1.100.0 <2.0.0"
  },
  "main": "./lib/main",
  "deserializers": {
    "JupyterNotebookEditor": "deserializeNotebookEditor"
  },
  "providedServices": {
    "jupyter": {
      "versions": {
        "1.0.0": "provideJupyter"
      }
    }
  },
  "dependencies": {
    "atom-select-list": "^0.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^5.3.11",
    "uuid": "^11.1.0"
  },
  "overrides": {
    "nan": "2.22.0",
    "uuid": "^11.1.0",
    "react": "^19.0.0",
    "react-is": "^19.0.0",
    "react-dom": "^19.0.0",
    "prismjs": "^1.29.0",
    "trim": "^1.0.1"
  },
  "package-deps": [
    "hydrogen-next"
  ],
  "configSchema": {
    "scrollOutput": {
      "order": 1,
      "title": "Scroll Long Output",
      "description": "Scroll output cells that exceed maximum height.",
      "type": "boolean",
      "default": true
    },
    "maxOutputHeight": {
      "order": 2,
      "title": "Max Output Height",
      "description": "Maximum height in pixels for output cells before scrolling.",
      "type": "integer",
      "default": 400,
      "minimum": 100,
      "maximum": 2000
    },
    "wordWrap": {
      "order": 4,
      "title": "Word Wrap",
      "description": "Wrap long lines in code cells.",
      "type": "boolean",
      "default": true
    },
    "clearOutputBeforeRun": {
      "order": 5,
      "title": "Clear Output Before Run",
      "description": "Clear cell output before running the cell.",
      "type": "boolean",
      "default": true
    }
  }
}
